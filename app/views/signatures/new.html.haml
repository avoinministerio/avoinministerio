.container_24
  #signature_form
    - @services.each do |service|
      .grid_16
        %form{:action => service[:url], :method => "post"}
          %input{:type => 'hidden', :name => 'A01Y_ACTION_ID',  :value => service[:action_id]}
          %input{:type => 'hidden', :name => 'A01Y_VERS',       :value => service[:vers]}
          %input{:type => 'hidden', :name => 'A01Y_RCVID',      :value => service[:rcvid]}
          %input{:type => 'hidden', :name => 'A01Y_LANGCODE',   :value => service[:langcode]}
          %input{:type => 'hidden', :name => 'A01Y_STAMP',      :value => service[:stamp]}
          %input{:type => 'hidden', :name => 'A01Y_IDTYPE',     :value => service[:idtype]}
          %input{:type => 'hidden', :name => 'A01Y_RETLINK',    :value => service[:retlink]}
          %input{:type => 'hidden', :name => 'A01Y_CANLINK',    :value => service[:canlink]}
          %input{:type => 'hidden', :name => 'A01Y_REJLINK',    :value => service[:rejlink]}
          %input{:type => 'hidden', :name => 'A01Y_KEYVERS',    :value => service[:keyvers]}
          %input{:type => 'hidden', :name => 'A01Y_ALG',        :value => service[:alg]}
          %input{:type => 'hidden', :name => 'A01Y_MAC',        :value => service[:mac]}
          %input{:type => 'submit', :value => service[:name]}
