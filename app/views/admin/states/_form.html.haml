= simple_form_for([:admin, @state]) do |f|
  = f.error_notification

  .inputs
    = f.hidden_field :administrator_id, value: current_administrator.id
    = f.hidden_field :city_id, value: @current_location.id
    = f.input :name
    = f.input :rank

  .actions
    = f.button :submit, class: 'btn btn-success'
